load("@aspect_rules_js//js:defs.bzl", "js_library")

js_library(
    name = "srcs",
    srcs = glob(
        ["**/*.ts", "**/*.tsx", "**/*.css"],
        exclude = ["**/*.test.ts", "**/*.test.tsx"],
    ),
    visibility = ["//frontend:__pkg__"],
